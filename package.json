{
  "name": "advanced-css-trillo",
  "version": "1.0.0",
  "description": "advances css and sass course Second project Flex",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "C:/Users/Ульяна/.node_modules_global/live-server.cmd",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css//style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulyavrubel/advanced-css-trillo.git"
  },
  "author": "Ulyana Sichkar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ulyavrubel/advanced-css-trillo/issues"
  },
  "homepage": "https://github.com/ulyavrubel/advanced-css-trillo#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
